#!/bin/Groovy
pipeline {
    agent any
    stages {
        stage('What is your name') {
           
            steps {
                echo 'My name is Abbey cheche'
                
            }
        }
        stage('Check date') {
           
            steps {
                sh 'date'
               
            }
        }
        stage('Check calendar') {
           
            steps {
                sh 'date'
               
            }
        }
         stage('Class') {
           
            steps {
                echo 'i love this class'
               
            }
        }
       
    }
}
